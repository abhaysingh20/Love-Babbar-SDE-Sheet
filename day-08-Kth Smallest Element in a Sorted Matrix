class Solution {
public:
    int kthSmallest(vector<vector<int>>& matrix, int k) {
        int n=matrix.size();
        priority_queue<int>v;
        
        for(int i=0;i<n;i++){
            for(int j=0;j<n;j++){
                v.push(matrix[i][j]);
                
                if(v.size()>k)
                    v.pop();
            }
        }
        return v.top();
        
    }
};
