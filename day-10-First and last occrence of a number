// { Driver Code Starts
#include<bits/stdc++.h>
using namespace std;

 // } Driver Code Ends
vector<int> find(int arr[], int n , int x )
{
    vector<int>v;
    int first=-1;
    int last=-1;
    for(int i=0;i<n;i++){
        if(arr[i]==x){
           first=i;
            break;
        }
    }
    for(int i=n-1;i>=0;i--){
        if(arr[i]==x)
        {
            last=i;
            break;
        }
    }
    v.push_back(first);
    v.push_back(last);
    return v;
}
