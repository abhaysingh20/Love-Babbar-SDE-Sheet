class Solution
{
    public:
    //Function to check if two strings are rotations of each other or not.
    bool areRotations(string s1,string s2)
    {
        // Your code here
           int n1 = s1.size(); int n2 = s2.size();
       if(n1!=n2) return false;
       s1= s1+s1;
       if(s1.find(s2)!=-1) return true;
       return false;
       
    }
};
