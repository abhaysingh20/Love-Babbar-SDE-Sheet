
class Solution{
public:
    int distinct(vector<vector<int>> m, int n)
    {
    unordered_map<int,int>mp;
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            if(mp[m[i][j]]==i)
            mp[m[i][j]]=i+1;
        }
    }
    int c=0;
    for(auto x:mp){
        if(x.second==n)
         c++;
    }
    return c;
    }
};
