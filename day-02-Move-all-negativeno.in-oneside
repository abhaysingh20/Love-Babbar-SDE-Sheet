class Solution{
    public:
    void segregateElements(int arr[],int n)
    {
        int a[n],j=0,k=0;
        for(int i=0;i<n;i++){
            if(arr[i]<0)
            a[j++]=arr[i];
            
        }
       for(int i=0;i<n;i++){
           if(arr[i]>=0)
           arr[k++]=arr[i];
       }
       j=0;
       for(int i=k;i<n;i++){
           arr[i]=a[j++];
       }
    }
};
